{
  "name": "webpack_react_demo",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "develop": "cd ./webpack && webpack-dev-server --env dev",
    "build": "cd ./webpack && webpack --env prod"
  },
  "config": {
    "dev": {
      "host": "localhost",
      "port": 8000
    },
    "prod": {
      "cdnHost": ""
    }
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Sam Mok <sammok2003@hotmail.com> (http://github.com/sammok)",
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router-dom": "^4.0.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.15.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.2.0",
    "webpack-merge": "^3.0.0"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^6.9.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
